<div class="container-fluid">
    <div class="row">
        <div class="col-md-2">
            <!--Sidebar content-->
            Search: <input ng-model="query">
        </div>
        <div class="col-md-10">
            <!--Body content-->

            <ul class="users">
                <li ng-repeat="user in users | filter:query">
                    <a href="#/phones/{{user.name}}">{{user.name}}</a>
                </li>
            </ul>
        </div>
    </div>
</div>